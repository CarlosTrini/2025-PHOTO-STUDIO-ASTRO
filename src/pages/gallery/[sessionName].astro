---
import LayoutGallery from "@layouts/LayoutGallery.astro";
import GalleryComponent from "@components/GalleryComponent.tsx";

export function getStaticPaths() {
    return [
        { params: { SessionName: "weddings" } },
        { params: { SessionName: "familia" } },
    ];
}

const { SessionName } = Astro.params;
---

<LayoutGallery>
    <GalleryComponent nameGallery={"weddings"} client:load />
</LayoutGallery>
